    <div class="container-sm position-relative border border-muted border border-3 w-75  mb-3 p-3 ps-5 pe-5"
        style="margin-top:80px; background-color: white;">
        <form action="/addinternship" method="POST" class="row g-3 form-container" enctype="multipart/form-data">
        <h3 style="text-align: center;">Internship</h3>
        <div class="row mb-3">
            <label for="name" class="col-sm-2 col-form-label">Name</label>
            <div class="col-sm-10">
              <input type="text" name="sname" class="form-control" id="name" required>
            </div>
        </div>
        <div class="row mb-3">
            <label for="name" class="col-sm-2 col-form-label">USN</label>
            <div class="col-sm-10">
              <input type="text" name="susn" class="form-control" id="usn">
            </div>
        </div>
        <div class="row mb-3">
            <label for="name" class="col-sm-2 col-form-label">Company Name</label>
            <div class="col-sm-10">
              <input type="text" name="cname" class="form-control" id="duration" step="any" placeholder=""required>
            </div>
        </div>
        <div class="row mb-3">
            <label for="name" class="col-sm-2 col-form-label">Duration</label>
            <div class="col-sm-10">
              <input type="number" name="dur" class="form-control" id="duration" step="any" placeholder="In months.."required>
            </div>
        </div>
        <div class="row mb-3">
            <label for="name" class="col-sm-2 col-form-label">Stipend</label>
            <div class="col-sm-10">
              <input type="number" name="stipend" class="form-control" id="Stipend" step="any">
            </div>
        </div>
        <div class="row mb-3">
            <label for="name" class="col-sm-2 col-form-label">Offer Letter</label>
            <div class="col-sm-10">
              <input type="file" name="ioffer" class="form-control" id=">OfferLetter" step="any" placeholder="In months.."required>
            </div>
        </div>
        <button type="submit"
            class="btn btn-primary position-relative start-50 botttom-0 translate-middle-x">Upload</button>
        </form>
    </div>
    <!-- <h3 style="text-align: center; ">List Of Circulars</h3>-->
    <div class="container-fluid w-50 mb-3">
        <form class="d-flex">
            <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
            <button class="btn btn-primary" type="submit">Search</button>
        </form>
    </div>
    <table class="table table-light table-striped table-hover table-bordered w-75 m-auto mb-4" id="abc">
        <thead>
            <tr>
                <th scope="col">SL</th>
                <th scope="col">Name of the Student</th>
                <th scope="col">USN</th>
                <th scope="col">Company Name</th>
                <th scope="col">Duration(in months)</th>
                <th scope="col">Stipend</th>
                <th scope="col">View</th>
                <th scope="col">Delete</th>
            </tr>
        </thead>
        <tbody>
            <% var i = 1%>
            <% for (let xg of xgs) { %>
            <tr>
                <th scope="row"><%=i++%></th>
                <td><%= xg.sname%></td>
                <td><%= xg.susn%></td>
                <td><%= xg.cname%></td>
                <td><%= xg.duration%></td>
                <td><%= xg.stipend%></td>
                <td><a href="<%= xg.ioffer%>" button type="button" class="btn btn-success btn-sm">View</a></td>
                <td><input type="button" class="btn btn-danger btn-sm" value="Delete"></input></td>
            </tr>
            <% } %>
        </tbody>
    </table>
   
