            <table class="table table-light table-striped table-hover table-bordered w-75 m-auto mb-4" id="abc">
                <thead>
                    <tr>
                        <th scope="col">SL</th>
                        <th scope="col">Name</th>
                        <th scope="col">Date</th>
                        <th scope="col">View</th>
                        <th scope="col">Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <% var k = 1%>
                    <% for (let sg of sgs) { %>
                    <tr>
                        <th scope="row"><%=k++%></th>
                        <td><%= sg.cname%></td>
                        <td><%= sg.cdate.toLocaleDateString()%></td>
                        <td><a href="<%= sg.clink%>" button type="button" class="btn btn-success btn-sm">View</a></td>
                        <td><input type="button" class="btn btn-danger btn-sm" value="Delete"></input></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
