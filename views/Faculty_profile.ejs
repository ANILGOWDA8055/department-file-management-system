                    <tr>
                        <th scope="row"><%=j++%></th>
                        <td><%= mg.cname%></td>
                        <td><%= mg.cdate.toLocaleDateString()%></td>
                        <td><a href="<%= mg.clink%>" button type="button" class="btn btn-success btn-sm">View</a></td>
                        <td><input type="button" class="btn btn-danger btn-sm" value="Delete"></input></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
            <h3 style="text-align: center; ">FDP Certficates</h3>
