    <body>
        <% for (let yg of ygs) { %>
        <div class="container-sm position-relative border border-muted border border-3 w-75  mb-3 p-3 ps-5 pe-5"
            style="margin-top:80px; background-color: white;">
            <section class="section about-section gray-bg" id="about">
                <form action="faculty_edit.html">
                    <div class="container">
                        <div class="row align-items-center flex-row-reverse">
                            <div class="col-lg-6">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <h6 class="mb-0">Full Name</h6>
                                        </div>
                                        <div class="col-sm-9 text-secondary">
                                            <%= yg.fname%>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <h6 class="mb-0">Designation</h6>
                                        </div>
                                        <div class="col-sm-9 text-secondary">
                                            <%= yg.fdesignation%>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <h6 class="mb-0">Faculty ID</h6>
                                        </div>
                                        <div class="col-sm-9 text-secondary">
                                            <%= yg.fid%>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <h6 class="mb-0">Email</h6>
                                        </div>
                                        <div class="col-sm-9 text-secondary">
                                            <%= yg.fmail%>
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <h6 class="mb-0">Mobile</h6>
                                        </div>
                                        <div class="col-sm-9 text-secondary">
                                            <%= yg.fphone%>
                                        </div>
                                    </div>
                                    <hr>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="card-body">
                                    <div class="d-flex flex-column align-items-center text-center">
                                        <img src="<%= yg.fphoto%>" alt="Admin"
                                            class="rounded-circle" width="200">
                                        <div class="mt-3">
                                            <h4><%= yg.fname%></h4>
                                            <p class="text-secondary mb-1">Associate Proffesor</p>
                                            <div class="d-grid gap-2 col-8 mx-auto">
                                                <td><a href="/profileedit/<%=yg.fid%>" button type="button" class="btn btn-primary btn-sm">Edit</a></td>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
            </section>
        </div>
        <% } %>
        <div class="container-sm position-relative border border-muted border border-3 w-75  mb-3 p-3 ps-5 pe-5"
            style="background-color: white;">
            <div class="container-fluid w-50 mb-3">
                <form class="d-flex">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-primary" type="submit">Search</button>
                </form>
            </div>
            <h3 style="text-align: center; ">Faculty Cerificates</h3>
            <hr>
            <table class="table table-light table-striped table-hover table-bordered w-75 m-auto mb-4" id="abc">
                <thead>
                    <tr>
                        <th scope="col">SL</th>
                        <th scope="col">Name</th>
                        <th scope="col">Date</th>
                        <th scope="col">View</th>
                        <th scope="col">Delete</th>
                    </tr>
                </thead>
                <tbody>
                    <% var i = 1%>
                    <% for (let lg of lgs) { %>
                    <tr>
                        <th scope="row"><%=i++%></th>
                        <td><%= lg.cname%></td>
                        <td><%= lg.cdate.toLocaleDateString()%></td>
                        <td><a href="<%= lg.clink%>" button type="button" class="btn btn-success btn-sm">View</a></td>
                        <td><input type="button" class="btn btn-danger btn-sm" value="Delete"></input></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
            <h3 style="text-align: center; ">Achievements</h3>
            <hr>
            <table class="table table-light table-striped table-hover table-bordered w-75 m-auto mb-4" id="abc">
                <thead>
                    <tr>
                        <th scope="col">SL</th>
                        <th scope="col">Name</th>
                        <th scope="col">Date</th>
                        <th scope="col">View</th>
                        <th scope="col">Delete</th>
                    </tr>
                </thead>
